<%- include('partials/header'); -%>
<h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="postitle" class=""">Title</label>      
            <input type="text" class="form-control" name="postitle" autocomplete="off">
        </div>
        <div class="form-group">
             <label for="postBody" class="">Post</label>
            <textarea name="postBody" class="form-control" id="" cols="30" rows="10"></textarea>
        </div>

        <button type="submit" class="btn btn-primary">Publish</button>
    </form>

<%- include('partials/footer'); -%>